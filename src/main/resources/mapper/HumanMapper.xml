<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mybatis.spring.main.app.mapper.HumanMapper">

    <select id="selectHumanById" resultType="mybatis.spring.main.app.entity.Human">
        SELECT id,name,height FROM human WHERE id=#{id}
    </select>
</mapper>